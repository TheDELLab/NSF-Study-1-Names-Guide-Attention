library(ggplot2)
library(dplyr)
# Assuming 'exported_datavyu' is your data frame
# Resetting the start of each trial to 0
data <- exported_datavyu %>%
group_by(trials.ordinal) %>%
mutate(
Adjusted_Onset = word_onset - trials.onset,
Adjusted_Offset = word_offset - trials.onset,
Label_Position = (Adjusted_Onset + Adjusted_Offset) / 2
) %>%
ungroup()
print(data)
# Filter for "hundred" and calculate averages and standard deviations
hundred_stats <- data %>%
filter(word == "hundred") %>%
summarise(
Avg_Onset = mean(Adjusted_Onset, na.rm = TRUE),
Avg_Offset = mean(Adjusted_Offset, na.rm = TRUE),
SD_Onset = sd(Adjusted_Onset, na.rm = TRUE),
SD_Offset = sd(Adjusted_Offset, na.rm = TRUE)
)
print(hundred_stats)
# Creating the Gantt Chart with added average times and error bars for "hundred"
gantt_chart <- ggplot(data, aes(y = trials.ordinal, x = Adjusted_Onset, xend = Adjusted_Offset, color = word)) +
geom_segment(aes(yend = trials.ordinal), size = 5) +
geom_text(aes(y = trials.ordinal, x = Label_Position, label = word), vjust = 0.5, size = 3, color = "gray22") +
geom_point(hundred_stats, aes(x = Avg_Onset, y = 1), color = "blue", size = 4) +  # Point for average onset
geom_point(hundred_stats, aes(x = Avg_Offset, y = 1), color = "red", size = 4) +  # Point for average offset
geom_errorbar(hundred_stats, aes(x = Avg_Onset, ymin = Avg_Onset - SD_Onset, ymax = Avg_Onset + SD_Onset, y = 2), color = "blue", width = 0.2) +
geom_errorbar(hundred_stats, aes(x = Avg_Offset, ymin = Avg_Offset - SD_Offset, ymax = Avg_Offset + SD_Offset, y = 2), color = "red", width = 0.2) +
theme_minimal() +
labs(x = "Time Since Trial Start (ms)", y = "Trial Number") +
theme(panel.grid.major = element_blank(), panel.grid.minor = element_blank())
library(ggplot2)
library(dplyr)
# Assuming 'exported_datavyu' is your data frame
# Resetting the start of each trial to 0
data <- exported_datavyu %>%
group_by(trials.ordinal) %>%
mutate(
Adjusted_Onset = word_onset - trials.onset,
Adjusted_Offset = word_offset - trials.onset,
Label_Position = (Adjusted_Onset + Adjusted_Offset) / 2
) %>%
ungroup()
# Calculate average onset and offset times for "hundred"
avg_onset_hundred <- mean(data$Adjusted_Onset[data$word == "hundred"], na.rm = TRUE)
avg_offset_hundred <- mean(data$Adjusted_Offset[data$word == "hundred"], na.rm = TRUE)
# Creating the Gantt Chart with lines for average times of "hundred"
gantt_chart <- ggplot(data, aes(y = trials.ordinal, x = Adjusted_Onset, xend = Adjusted_Offset, color = word)) +
geom_segment(aes(yend = trials.ordinal), size = 5) +
geom_text(aes(y = trials.ordinal, x = Label_Position, label = word), vjust = 0.5, size = 3, color = "gray22") +
geom_hline(yintercept = avg_onset_hundred, color = "blue", linetype = "dashed", size = 1) +  # Line for average onset
geom_hline(yintercept = avg_offset_hundred, color = "red", linetype = "dashed", size = 1) +  # Line for average offset
theme_minimal() +
labs(x = "Time Since Trial Start (ms)", y = "Trial Number") +
theme(panel.grid.major = element_blank(), panel.grid.minor = element_blank())
# Display the plot
print(gantt_chart)
